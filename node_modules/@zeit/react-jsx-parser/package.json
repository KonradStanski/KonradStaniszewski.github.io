{
  "author": "Troy Alford",
  "bugs": "https://github.com/TroyAlford/react-jsx-parser/issues",
  "description": "A React component which can parse JSX and output rendered React Components",
  "keywords": [
    "react",
    "jsx"
  ],
  "license": "MIT",
  "main": "lib/react-jsx-parser.min.js",
  "name": "@zeit/react-jsx-parser",
  "repository": "TroyAlford/react-jsx-parser",
  "version": "2.0.0",
  "dependencies": {
    "acorn-jsx": "^4.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "cross-env": "^5.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "eslint": "^5.15.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "jest": "^24.1.0",
    "jest-cli": "^24.1.0",
    "mkdirp": "^0.5.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "source-map-explorer": "^1.7.0",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3"
  },
  "peerDependencies": {
    "react": "^16.8.3",
    "react-dom": "^16.8.3"
  },
  "resolutions": {
    "extend": "^3.0.2",
    "merge": "^1.2.1"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack",
    "postversion": "git push origin HEAD && git push origin HEAD --tags",
    "prebuild": "mkdirp ./lib && rm -rf ./lib/*",
    "preversion": "yarn test",
    "sourcemap": "yarn build && source-map-explorer ./lib/react-jsx-parser.min.js",
    "test": "yarn jest",
    "version": "yarn build && git add -A ./lib"
  },
  "jest": {
    "collectCoverageFrom": [
      "source/**/*.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>setupTests.js"
    ],
    "transform": {
      ".js$": "babel-jest"
    }
  }
}
